side1,side2, side3=map(int,input().split())
from math import acos, degrees
angle1=acos((side2**2 + side3**2 - side1**2)/(2*side2*side3))
angle2=acos((side1**2 + side3**2 - side2**2)/(2*side1*side3))
angle3=acos((side2**2 + side1**2 - side3**2)/(2*side2*side1))
print(degrees(angle1))
print(degrees(angle2))
print(degrees(angle3))
